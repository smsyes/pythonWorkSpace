QComboBox::item {
border: 0px solid black;
min-height: 35px; 
}


/* #Items */
QListView::item {
min-height: 35px; 
border: solid;
border-width:1;
}




/* Button when open*/

QComboBox:!editable:on, QComboBox::drop-down:editable:on {
background-color: rgb(0, 0, 0,50);
color: rgb(200, 200, 200);
padding-left: 4px;

}

/* Normal arrow look*/

QComboBox::down-arrow
{
	margin-top: 4px;
	border-left: 7px solid rgb(27, 42, 50,0);
	border-right: 7px solid rgb(27, 42, 50,0);
	border-bottom: 7px solid rgb(27, 42, 50,0);
	border-top: 6px solid  rgb(200,200, 200);
}

/* Arrow on open*/

QComboBox:down-arrow:on
{
	margin-bottom: 4px;
	border-left: 7px solid rgb(27, 42, 50,0);
	border-right: 7px solid rgb(27, 42, 50,0);
	border-top: 7px solid rgb(27, 42, 50,0);
	border-bottom: 6px solid  rgb(200,200, 200);
}

/* Arrow background */

QComboBox::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
	padding-right: 10px;
border-bottom-left-radius: 5px;
border-top-left-radius: 5px;
border-bottom-right-radius: 0px;
border-top-right-radius: 0px;
}




